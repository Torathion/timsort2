    "typestar": "^1.8.0"
  },
  "devDependencies": {
    "@rollup/rollup-win32-ia32-msvc": "^4.35.0",
    "@types/node": "^22.13.10",
    "@vitest/coverage-v8": "^3.0.8",
    "cli-table3": "^0.6.5",
    "esbuild": "^0.25.1",
    "eslint": "^9.22.0",
    "eslint-config-shiny": "^4.1.0",
    "timsort": "^0.3.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
